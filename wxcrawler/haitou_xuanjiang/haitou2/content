<script type="text/javascript" src="/js/article.js"></script>
<script type="text/javascript" src="/js/ajax.js"></script>
<script type="text/javascript">

	var KzWebHeight = 0;  //#kz-web内所有div的高度和

	$(document).bind("ready", function(){
		$('#kz-web').children().each(function(){
			KzWebHeight += $(this).height();
		});

		$('#kz-web').scroll(function(){
			if((KzWebHeight+$('.navbar-static-top').height()-$('#kz-web').scrollTop()-$(this).height()) < $('.footer').height() - 150){
				//$('.xf-div').addClass('fadeOut');
				$('.xf-div').fadeOut();
			}
			else
				//$('.xf-div').removeClass('fadeOut');
				$('.xf-div').fadeIn();
		});

		//快照切换
		$('.has-switch').click(function(){
			if($('#frameUrl')[0].value == ''){
				alert('抱歉！该信息没有原站模式');  //可以以更优雅的方式提示 待改进
				return;
			}
			$(this).find('.switch-animate').toggleClass('switch-on').toggleClass('switch-off');
			if($(this).find('.switch-animate').hasClass('switch-on')){
				if($('#yz-web iframe').length < 1){
					var iframe = $('<iframe frameborder="0" id="web"></iframe>');
					iframe[0].src = $('#frameUrl')[0].value;
					iframe.bind("load", function(){
						$('#yz-web i').fadeOut();
					});
					$('#yz-web').append(iframe);
					//iframe.height($(window).height()-$('.navbar-static-top').height()-4);
					//只在初始化时做iframe大小调整 会导致在初始化后resize就会出bug 待修改
				}
				$('#yz-web').fadeIn();  //适应IE
			}else if($(this).find('.switch-animate').hasClass('switch-off')){
				$('#yz-web').fadeOut();  //适应IE
			}
		});
	});

</script>
